{
  "name": "cmi-acol",
  "version": "0.1.0",
  "description": "A Course of Love in the Library of Christ Mind Teachings",
  "scripts": {
    "base:dev:webpack": "webpack --mode development",
    "base:dev:jekyll": "JEKYLL_ENV=development bundle exec jekyll build --config ./_config.prod.yml",
    "base:prod:webpack": "webpack --mode production",
    "base:prod:jekyll": "JEKYLL_ENV=production bundle exec jekyll build --config ./_config.prod.yml",
    "check:dev": "JEKYLL_ENV=development bundle exec jekyll build",
    "check:prod": "JEKYLL_ENV=production bundle exec jekyll build",
    "watch": "webpack --mode development --watch",
    "jekyll": "JEKYLL_ENV=development bundle exec jekyll s",
    "start": "npm run jekyll & npm run watch"
  },
  "keywords": [
    "cmi",
    "acol",
    "jekyll"
  ],
  "author": "Rick Mercer",
  "license": "MIT",
  "homepage": "https://www.christmind.info/t/acol",
  "repository": "https://github.com/Christ-Mind-Teachings/cmi-audio-plugins.git",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.2",
    "css-loader": "^2.1.1",
    "eslint": "^6.8.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "axios": "^1.3.6"
  }
}
