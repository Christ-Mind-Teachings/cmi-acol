{
  "name": "search",
  "version": "1.0.0",
  "description": "Index of ACIM content for CMI Search",
  "main": "load",
  "arg": {
    "manual": "../manual/*.json",
    "workbook": "../workbook/*.json",
    "text": "../text/*.json",
    "table": "raj"
  },
  "scripts": {
    "compile": "babel --presets es2015 src --out-dir bin",
    "desc:l": "node bin/describe_table -e local -t $npm_package_arg_table",
    "desc:r": "node bin/describe_table -e remote -t $npm_package_arg_table",
    "del:l": "node bin/delete_table -e local -t $npm_package_arg_table",
    "del:r": "node bin/delete_table -e remote -t $npm_package_arg_table",
    "create:l": "node bin/create_table -e local -t $npm_package_arg_table",
    "create:r": "node bin/create_table -e remote -t $npm_package_arg_table",
    "load:l:manual": "node bin/load -e local $npm_package_arg_manual > logs/manual_local.log",
    "load:l:workbook": "node bin/load -e local $npm_package_arg_workbook > logs/workbook_local.log",
    "load:l:text": "node bin/load -e local $npm_package_arg_text > logs/text_local.log",
    "load:r:manual": "node bin/load -e remote $npm_package_arg_manual > logs/manual_remote.log",
    "load:r:workbook": "node bin/load -e remote $npm_package_arg_workbook > logs/workbook_remote.log",
    "load:r:text": "node bin/load -e remote $npm_package_arg_text > logs/text_remote.log"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  },
  "dependencies": {
    "aws-sdk": "^2.231.1",
    "commander": "^2.15.1"
  }
}
